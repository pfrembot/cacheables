{
    "name": "pfrembot/cacheables",
    "type": "library",
    "description": "Abstraction library for caching hierarchical data structures",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pfrembot\\Cacheables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pfrembot\\Cacheables\\Tests\\": "tests/"
        },
        "files": [
            "tests/_mocks/classes.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pfrembot/identity"
        }
    ],
    "require": {

    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "epfremme/collections": "^1.0",
        "pfrembot/identity": "^1.0",
        "symfony/cache": "^3.1"
    },
    "config": {
        "bin-dir": "bin"
    }
}
